msgid ""
"data/events.xml.append$/dummyroot/FTL/event[@name=\"LILY_EMPORIUM_AGAIN\"]/choice[@req=\"LILY_PATRONAGE\"]/text"
msgstr "Demander à Lily de vous rejoindre."

#: src-en/data/events.xml.append:469
msgid ""
"data/events.xml.append$/dummyroot/FTL/event[@name=\"LILY_EMPORIUM_AGAIN\"]/choice[@req=\"LILY_PATRONAGE\"]/event/text"
msgstr ""
"\"Alors vous avez changé d’avis, après tout ?\n"
"Très bien, je vous accompagne...\""

#: src-en/data/events.xml.append:474
msgid ""
"data/events.xml.append$/dummyroot/FTL/event[@name=\"LILY_EMPORIUM_AGAIN\"]/choice[@req=\"LILY_PATRONAGE\"]/event/choice/text"
msgstr "Continuer..."

