    <mod:findLike type="weaponBlueprint">
        <!-- mark the statBoost -->
        <mod:findLike type="statBoosts">
            <mod:findWithChildLike type="statBoost" child-type="boostAnim">
                <mod:selector>madness</mod:selector>
                <mod:setAttributes lilys="beam_emporium" />
            </mod:findWithChildLike>
        </mod:findLike>
        <!-- find the statBoosts via the marked statBoost -->
        <mod:findWithChildLike type="statBoosts" child-type="statBoost">
            <mod:selector lilys="beam_emporium" />
            <mod:findLike>            <!-- every statBoost -->
                <mod:findLike type="blackList">
                    <mod-append:unique_lily_avatar/>
                </mod:findLike>
            </mod:findLike>
            <mod:findLike>            <!-- the marked statBoost -->
                <mod:selector lilys="beam_emporium" />
                <mod:removeAttributes lilys="" />
            </mod:findLike>
        </mod:findWithChildLike>
    </mod:findLike>

