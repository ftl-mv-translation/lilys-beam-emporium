msgstr "(韻を踏む者) 数えさせてもらおう：3、2、1…"

#: src-en/data/events_special_storage.xml.append:568
msgid ""
"data/events_special_storage.xml.append$/dummyroot/FTL/event[@name=\"STORAGE_CHECK_LILY_JOIN_FIRST\"]/choice[@req=\"eldritch_cat\"]/event/text"
msgstr ""
